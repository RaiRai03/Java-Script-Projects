{
    "name": "Motivational Reminder",
    "version": "1.0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "icons": {
      "128": "logo.png"
    },
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"]
    }
  }